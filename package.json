  "description": "Ancel Ajanga - Fullstack Software Engineer Portfolio",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate-seo && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"pages/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "preview": "npm run build && next start",
    "deploy": "npm run build && npx netlify deploy --prod --dir=out",
    "generate-seo": "node scripts/generate-sitemap.js && node scripts/generate-robots.js && node scripts/generate-sitemap-for-ai.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "generate-sitemap-for-ai": "node scripts/generate-sitemap-for-ai.js",
    "generate-robots": "node scripts/generate-robots.js",
    "validate-seo": "node scripts/validate-seo.js",
    "seo-check": "npm run generate-seo && npm run validate-seo",
    "minify-jsonld": "node scripts/minify-jsonld.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
