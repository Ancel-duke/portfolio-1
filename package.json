    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-helmet-async": "^1.3.0",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^2.12.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^2.1.4"
